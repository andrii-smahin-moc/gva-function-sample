{
  "name": "gva-sample-function",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsup && mv ./dist/function.js ./$npm_package_name.js",
    "dev": "wrangler dev --port=3000 --inspector-port=9229",
    "format": "prettier --write --log-level=warn '**/*.ts' && eslint --cache --fix ./src",
    "test": "vitest run",
    "coverage": "vitest --config vitest.coverage.config.ts run --coverage && node generate-coverage-badge.js",
    "lint": "eslint --cache ./src && prettier --check '**/*.ts'",
    "generate-types": "wrangler types",
    "typecheck": "npx tsc --noEmit",
    "postinstall": "cd ../.. && npm install --no-audit --no-fund || true"
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "^0.8.56",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@types/node": "^22.8.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.41.0",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^7.0.3",
    "deepmerge": "^4.3.1",
    "eslint": "^9.31.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-project-structure": "^3.13.3",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unicorn": "^60.0.0",
    "globals": "^15.11.0",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "type-fest": "^4.30.2",
    "typed-dotenv": "^10.0.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.12.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "wrangler": "^3.114.13"
  },
  "overrides": {
    "eslint-config-airbnb-base": {
      "eslint": "^9"
    }
  },
  "dependencies": {
    "valibot": "^1.1.0"
  }
}
